{
  "compilerOptions": {
    "jsx": "react-native",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": false,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "incremental": true,
    "outDir": "./lib",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true
  },
  "exclude": ["node_modules", "lib", "**/**/*/__tests__", "dist"],
  "include": ["./src/components/shared"],
  "files": ["environment.d.ts"]
}
